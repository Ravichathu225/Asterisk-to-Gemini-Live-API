# External AI WebSocket Configuration
AI_WEBSOCKET_URL=wss://your-websocket-url.com/ws
AI_AUTH_TOKEN=your_optional_auth_token_here
AI_VOICE=default
AI_LANGUAGE=en

# Asterisk ARI Configuration
ARI_URL=http://127.0.0.1:8088
ARI_USERNAME=asterisk
ARI_PASSWORD=your_ari_password

# Call Management
MAX_CONCURRENT_CALLS=10
CALL_DURATION_LIMIT_SECONDS=300

# Logging
LOG_LEVEL=info

# AI Behavior Configuration
SYSTEM_PROMPT="Your AI assistant prompt here"
INITIAL_MESSAGE=Hi

# Audio Configuration
SILENCE_PADDING_MS=100

# Voice Activity Detection (VAD)
VAD_ENABLED=true
VAD_THRESHOLD=0.6
VAD_PREFIX_PADDING_MS=200
VAD_SILENCE_DURATION_MS=600

# Tinybird Configuration
DATASOURCE_NAME=otrix_ai_call_logs
TINYBIRD_HOST=https://api.tinybird.co
TINYBIRD_API_TOKEN=your_tinybird_api_token_here

# Bubble Database and API Configuration
BUBBLE_API_TOKEN=your_bubble_api_token_here
BUBBLE_API_ENDPOINT_URL=https://your-bubble-app.com/api/1.1/wf
BUBBLE_VERSION_TEST_ENDPOINT_URL=https://your-bubble-app.com/version-test/api/1.1/wf

# Bubble Database Configuration
BUBBLE_API_DATABASE_URL=https://your-bubble-app.com/version-test/api/1.1/obj

# FreePBX/Asterisk AMI Configuration (for automated setup)
AMI_HOST=localhost
AMI_PORT=5038
AMI_USERNAME=admin
AMI_SECRET=your_ami_secret_here
PROVIDER_SIP_SERVER=sip.your-provider.com:5060
API_HOST=your.api.host

# FreePBX Setup API Configuration
FREEPBX_SETUP_PORT=3000
